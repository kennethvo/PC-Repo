<?xml version="1.0" encoding="UTF-8"?>
<jmeterTestPlan version="1.2" properties="5.0" jmeter="5.6.3">
  <hashTree>
    <TestPlan guiclass="TestPlanGui" testclass="TestPlan" testname="ELK Demo Load Test" enabled="true">
      <stringProp name="TestPlan.comments">Load test for Item Service and User Service to generate logs for ELK demo</stringProp>
