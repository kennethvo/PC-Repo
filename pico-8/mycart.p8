__lua__
function _init()
	px=63
	py=63
	button=""
	
end

function _update()
	if btn(➡️) then
		px+=1
	end
	if btn(⬅️) then
		px-=1
	end
	if btn(⬆️) then
		py-=1
	end
	if btn(⬇️) then
		py+=1
	end
end

function _draw()
	cls()
	spr(1,px,py)
	print(button)
end
